{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Run (CosmosBulkDemo)",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}/src/bin/Debug/net9.0/CosmosBulkDemo.dll",
      "preLaunchTask": "build",
      "cwd": "${workspaceFolder}/src",
      "env": {
        "COSMOS_ENDPOINT": "<COSMOS_DB_ENDPOINT>",
        "COSMOS_KEY": "<COSMOS_KEY>",
        "COSMOS_DB": "demoDB",
        "COSMOS_CONTAINER": "orders",
        "LOAD_DOCS": "10000",
        "LOAD_CONCURRENCY": "32",
        "LOAD_USERS": "2000"
      }
    }
  ]
}
